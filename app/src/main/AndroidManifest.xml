<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tabbed_test">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tabbed_test">
        <activity android:name=".Registration" android:theme="@style/Theme.Tabbed_test_NoActionBar">

        </activity>

        <activity android:name=".LoginPage" android:theme="@style/Theme.Tabbed_test_NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".NewEntry"
            android:theme="@style/Theme.Tabbed_test_NoActionBar">

        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Tabbed_test_NoActionBar" >

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>